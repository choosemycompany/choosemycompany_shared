{
    "name": "choosemycompany/shared",
    "description": "ChooseMyCompany Shared Context",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "type": "library",
    "autoload": {
        "psr-4": {
            "ChooseMyCompany\\Shared\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChooseMyCompany\\Shared\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "cmc_tech"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-bcmath": "*",
        "beberlei/assert": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpro/grumphp-shim": "*",
        "phpunit/phpunit": "*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "phpro/grumphp-shim": true
        }
    }
}
